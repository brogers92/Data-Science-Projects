    "### Step 5:  Merge CSV files into one large file"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 108,
   "id": "d06818d1",
   "metadata": {},
   "outputs": [],
   "source": [
    "# list of CSV files to merge\n",
    "baseball_data_paths = [\"1997 MLB Pitching Statistics.csv\", \"1997 MLB Batting Statistics.csv\"]\n",
    "\n",
    "# initialize DataFrames for each file\n",
    "dfs = [pd.read_csv(baseball_data_path) for baseball_data_path in baseball_data_paths]\n",
    "\n",
    "# merge DataFrames based on a common key, 'Name'\n",
    "merged_baseball_df = pd.merge(dfs[0], dfs[1], on='Name', how='inner')\n",
    "\n",
    "# save the merged DataFrame to a new CSV file\n",
    "merged_baseball_df.to_csv('1997 Baseball Statistics.csv', index=False)"
   ]
  },
